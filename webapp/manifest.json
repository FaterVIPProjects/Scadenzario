{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "org.fater.scadenzario",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZVIP_PROJECT_SRV": {
				"uri": "/sap/opu/odata/sap/ZVIP_PROJECT_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZVIP_PROJECT_SRV/metadata.xml"
				}
			},
			"ZVIP_USERMGMT_SRV": {
				"uri": "/sap/opu/odata/sap/ZVIP_USERMGMT_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZVIP_USERMGMT_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "org.fater.scadenzario.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "org.fater.scadenzario.i18n.i18n"
				}
			},
			"oDataModel": {
				"type": "sap.ui.model.odata.ODataModel",
				"dataSource": "ZVIP_PROJECT_SRV",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "None",
					"headers": {
						"X-Requested-With": "X",
						"Content-Type": "application/json"
					}
				}
			},
			"oUserModel": {
				"type": "sap.ui.model.odata.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "None",
					"headers": {
						"X-Requested-With": "X",
						"Content-Type": "application/json"
					}
				},
				"dataSource": "ZVIP_USERMGMT_SRV"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zscadenzario/webapp",
		"_version": "1.1.0"
	}
}